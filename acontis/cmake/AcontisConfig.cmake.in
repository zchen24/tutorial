# - Config file for the Acontis package
# It defines the following variables
#  Acontis_INCLUDE_DIR  - include directories for Acontis
#  Acontis_LIBRARY_DIR  - link library directories for Acontis
#  Acontis_LIBRARIES    - libraries to link against

# Version
set (Acontis_VERSION_MAJOR "@Acontis_VERSION_MAJOR@")
set (Acontis_VERSION_MINOR "@Acontis_VERSION_MINOR@")
set (Acontis_VERSION_SERVICEPACK "@Acontis_VERSION_SERVICEPACK@")
set (Acontis_VERSION_BUILD "@Acontis_VERSION_BUILD@")
set (Acontis_VERSION "@Acontis_VERSION@")

# Compute paths
set (Acontis_INCLUDE_DIR "@CONF_INCLUDE_DIR@")
set (Acontis_LIBRARY_DIR "@CONF_LIBRARY_DIR@")

# Libraries to link against
set (Acontis_LIBRARIES "@CONF_LIBRARIES@")
